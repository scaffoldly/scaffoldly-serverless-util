{
  "name": "@scaffoldly/serverless-util",
  "version": "1.0.7",
  "description": "Serverless Utility Functions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push origin HEAD:master && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scaffoldly/scaffoldly-serverless-util.git"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "Serverless",
    "Utility",
    "Util",
    "Functions"
  ],
  "license": "GPL-3.0-or-later",
  "author": "Christian Nuss",
  "bugs": {
    "url": "https://github.com/scaffoldly/scaffoldly-serverless-util/issues"
  },
  "homepage": "https://github.com/scaffoldly/scaffoldly-serverless-util#readme",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/node": "^14.0.5",
    "@types/uuid": "^8.0.0",
    "aws-sdk": "^2.682.0",
    "aws-xray-sdk": "^3.2.0"
  }
}
